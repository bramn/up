# Ignore the folder with documentation, which is mounted later on using docker compose
/docs

# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep
/coverage/

# Ignore packs generated by webpack
/public/packs
/public/packs-test
/public/assets
/public/javascripts/*

# Ignore all Javascript modules installed by and stuff generated by Yarn
/node_modules
yarn-*.log*
.yarn-integrity

# Coverage reports about tests.
coverage

# Ignore keys for decrypting credentials and more.
/config/*.key
/config/credentials/*.key
/config/database.yml
/db/*.sqlite3

/public/packs
/public/packs-test
/node_modules
yarn-error.log
yarn-debug.log*
.yarn-integrity

# IDE stuff, this should actually be ignored in your own global .gitignore
*.iml
